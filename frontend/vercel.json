{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://cyprusforchrist-production.up.railway.app/api/$1"
        },
        {
            "source": "/media/(.*)",
            "destination": "https://cyprusforchrist-production.up.railway.app/media/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}